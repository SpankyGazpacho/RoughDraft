def validatelist(seplist, listtype, errorcodelist):
    numofentries = len(seplist)
    x = 0
    try:
        if listtype == "i":
            seplist = list(map(int, seplist))
        elif listtype != "s":
            errorcodelist.append(1)
    except(ValueError):
        errorcodelist.append(2)
    print("Val done")
    return (seplist, errorcodelist)
